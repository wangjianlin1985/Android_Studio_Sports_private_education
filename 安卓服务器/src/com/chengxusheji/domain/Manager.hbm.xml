<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="Manager" table="Manager">

          <!-- 主键管理员用户名 -->
          <id name="managerUserName" type="string" length="20">
            <generator class="assigned"/>
          </id>

          <!-- 简单字符串属性: 登录密码 -->
          <property
            name="password"
            column="password"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 姓名 -->
          <property
            name="name"
            column="name"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 性别 -->
          <property
            name="sex"
            column="sex"
            type="string"
            length="4"
          />

          <!-- 简单日期型属性: 出生日期 -->
          <property
            name="birthDate"
            column="birthDate"
            	type="timestamp"
            	length="10"
          />

          <!-- 简单字符串属性: 联系电话 -->
          <property
            name="telephone"
            column="telephone"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
