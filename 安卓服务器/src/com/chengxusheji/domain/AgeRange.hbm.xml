<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="AgeRange" table="AgeRange">

          <!-- 主键年龄范围id -->
          <id name="arId" column="arId">
            <generator class="native"/>
          </id>

          <!-- 简单整型属性: 开始年龄 -->
          <property
            name="startAge"
            column="startAge"
            type="int"

          />

          <!-- 简单整型属性: 结束年龄 -->
          <property
            name="endAge"
            column="endAge"
            type="int"

          />

          <!-- 简单字符串属性: 显示信息 -->
          <property
            name="showText"
            column="showText"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
