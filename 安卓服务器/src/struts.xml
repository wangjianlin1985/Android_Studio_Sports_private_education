     <!--Struts2中Action对象的创建交给spring完成-->
     <constant name="struts.objectFactory" value="spring" />   
       
          
	 <package name="login" namespace="/login" extends="struts-default">
	        <action name="login_*" class="loginAction" method="{1}">
	        	<result name="main_view">/main.jsp</result>
	        	<result name="error">/error.jsp</result>
	        	<result name="input">/login.jsp</result> 
	        	<result name="login_view">/login.jsp</result>
	          
	        </action>  
	 </package>
	 
         <!-- CoachAction配置 -->
         <package name="Coach" namespace="/Coach" extends="struts-default">
           <action name="Coach_*" class="coachAction" method="{1}">
             <result name="add_view">/Coach_add.jsp</result>
             <result name="input">/Coach_add.jsp</result>
             <result name="add_success">/message.jsp</result>
             <result name="delete_success">/message.jsp</result>
             <result name="modify_success">/message.jsp</result>
             <result name="error">/error.jsp</result>
             <result name="query_view">/Coach_query_result.jsp</result>
             <result name="front_query_view">/Coach_frontquery_result.jsp</result>
             <result name="front_show_view">/Coach_frontshow.jsp</result>
             <result name="modify_view">/Coach_modify.jsp</result>
           </action>
         </package>

